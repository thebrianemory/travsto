<div class="container">
  <div class="col-md-10">
    <h2><%= "Travel Stories by " + @user.username %></h2>
    <% @my_trips.each do |trip| %>
      <div class="well well-white">
        <h4><%= link_to trip.title, trip_path(trip) %></h4>
        <p><%= truncate(trip.description, length: 250) %> <%= link_to "Read More", trip_path(trip) %></p>
      </div>
    <% end %>
  </div>
</div>
